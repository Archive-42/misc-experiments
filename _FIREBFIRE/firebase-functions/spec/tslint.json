{
  "extends": "tslint:recommended",
  "rules": {
    "quotemark": [true, "single", "avoid-escape"],
    "interface-name": [false],
    "variable-name": [true, "check-format", "allow-leading-underscore"],
    "object-literal-sort-keys": false,
    "whitespace": [true],
    "member-access": [false],
    "no-string-literal": false,
    "no-console": [false],
    "no-namespace": [false]
  }
}
